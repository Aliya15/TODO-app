{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
      "project": ["tsconfig.json"]
    },
    "rules": {
      "semi": ["error", "always"],
      "no-extra-semi": "off",
      "quotes": ["error", "single"],
      "no-unused-expressions": "error",
      "consistent-return": [
        "warn"
      ]
    }
}
